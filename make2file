	gcc -c inputData/inputDataConsole.c

outputDataConsole.o : outputData/outputDataConsole.c
	gcc -c outputData/outputDataConsole.c

addTechPrefix.o : my_lib/addTechPrefix.c
	gcc -c my_lib/addTechPrefix.c

cpyElemStruct.o : my_lib/cpyElemStruct.c
	gcc -c my_lib/cpyElemStruct.c

freeNamesFields.o : my_lib/freeNamesFields.c
	gcc -c my_lib/freeNamesFields.c

includeElemByIndex.o : my_lib/includeElemByIndex.c
	gcc -c my_lib/includeElemByIndex.c

newElement.o : my_lib/newElement.c
	gcc -c my_lib/newElement.c

structNewMem.o: my_lib/structNewMem.c
	gcc -c my_lib/structNewMem.c


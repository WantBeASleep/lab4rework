lab4.o : main.o inputMenu.o mainMenu.o outputMenu.o inputDataConsole.o outputDataConsole.o addTechPrefix.o cpyElemStruct.o freeNamesFields.o includeElemByIndex.o newElement.o structNewMem.o
	gcc -o lab4 main.o inputMenu.o mainMenu.o outputMenu.o inputDataConsole.o outputDataConsole.o addTechPrefix.o cpyElemStruct.o freeNamesFields.o includeElemByIndex.o newElement.o structNewMem.o -lreadline
	rm *.o

main.o : main.c
	gcc -c main.c

inputMenu.o : menuPrintScanf/inputMenu.c
	gcc -c menuPrintScanf/inputMenu.c

mainMenu.o : menuPrintScanf/mainMenu.c
	gcc -c menuPrintScanf/mainMenu.c

outputMenu.o : menuPrintScanf/outputMenu.c
	gcc -c menuPrintScanf/outputMenu.c

